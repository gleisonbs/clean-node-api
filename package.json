  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.6.6",
    "@types/node": "^14.14.25",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "4",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "yarn build && docker-compose up --build -d",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:verbose": "jest --verbose false --silent=false --passWithNoTests --runInBand",
    "test:integration": "yarn test --watchAll -c jest.integration.config.js",
    "test:unit": "yarn test --watchAll -c jest.unit.config.js",
    "test:staged": "yarn test",
    "test:ci": "yarn test --coverage"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "fast-glob": "^3.2.5",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.4",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "validator": "^13.5.2"
  },
  "engines": {
    "node": "12.x"
  }
}
